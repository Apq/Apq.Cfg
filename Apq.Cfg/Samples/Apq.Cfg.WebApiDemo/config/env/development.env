# 开发环境配置覆盖
# 这些值会覆盖 base 目录中的配置

APP_NAME=Apq.Cfg WebApi Demo (Development)
APP_DEBUG=true

# 日志级别
LOGGING_LOGLEVEL_DEFAULT=Debug
LOGGING_LOGLEVEL_MICROSOFT_ASPNETCORE=Information

# 服务器配置
SERVER_HOST=localhost
SERVER_PORT=5000
SERVER_USEHTTPS=false

# 数据库
DATABASE_PRIMARY_CONNECTIONSTRING=Server=localhost;Database=ApqCfgDemo_Dev;Trusted_Connection=True;TrustServerCertificate=True

# 缓存
CACHE_PROVIDER=Memory
CACHE_MEMORY_SIZELIMIT=256

# 安全
SECURITY_ENABLEHTTPS=false
SECURITY_CORS_ALLOWANYORIGIN=true
SECURITY_RATELIMIT_ENABLED=false
